{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "arrowParens": "always",
  "tabWidth": 2,
  "semi": false,
  "importOrder": [
    "react/(.*)$",
    "^@mui/(.*)$",
    "^@emotion/(.*)$",
    "^@iconify/(.*)$",
    "^pages/(.*)$",
    "^components/(.*)$",
    "^api/(.*)$",
    "^hooks/(.*)$",
    "^utils/(.*)$",
    "^redux/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true
}
